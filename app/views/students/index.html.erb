<h2>search students by name</h2>
<%= form_tag students_path, method: 'get' do %>
  <%= label_tag 'enter a name:'%>
  <%= text_field_tag :search, params[:search] %><br>
  <%= submit_tag 'search' %>
<% end %>
<% @students.each do |student| %>
  <%= render partial: 'student', locals: {student: student} %>
<% end %>